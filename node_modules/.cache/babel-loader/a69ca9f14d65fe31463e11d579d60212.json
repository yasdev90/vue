SAAA,GAAA,CAAA,EAAA;AACA,aAAA,SAAA;AACA,aAAA,aAAA,IAAA,QAAA,CAAA,KAAA,YAAA,CAAA;AACA,aAAA,YAAA;AACA;AACA,KAjBA;AAkBA,IAAA,YAAA,EAAA,wBAAA;AACA,WAAA,cAAA,GAAA,KAAA,YAAA,CAAA,KAAA,CAAA,KAAA,aAAA,EAAA,QAAA,CAAA,KAAA,aAAA,CAAA,GAAA,QAAA,CAAA,KAAA,YAAA,CAAA,CAAA;AACA,KApBA;AAqBA,IAAA,WAAA,EAAA,qBAAA,QAAA,EAAA;AACA,WAAA,YAAA,GAAA,QAAA;AACA,WAAA,aAAA,GAAA,CAAA;AACA,WAAA,SAAA,GAAA,CAAA;AACA,WAAA,YAAA;AACA,KA1BA;AA2BA,IAAA,eA3BA,6BA2BA;AACA,WAAA,YAAA,GAAA,KAAA,SAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,CAAA,IAAA,CAAA,SAAA;AAAA,OAAA,CAAA;AACA,WAAA,YAAA;AACA,KA9BA;AA+BA,IAAA,kBA/BA,gCA+BA;AACA,WAAA,YAAA,GAAA,KAAA,SAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,SAAA;AAAA,OAAA,CAAA;AACA,WAAA,YAAA;AACA,KAlCA;AAmCA,IAAA,YAnCA,0BAmCA;AACA,WAAA,YAAA,GAAA,KAAA,SAAA;AACA,WAAA,YAAA;AACA,KAtCA;AAuCA,IAAA,cAvCA,4BAuCA;AACA,aAAA,KAAA,WAAA,GAAA,CACA;AACA,QAAA,IAAA,EAAA,KADA;AAEA,QAAA,KAAA,EAAA,KAAA,SAAA,CAAA;AAFA,OADA,EAKA;AACA,QAAA,IAAA,EAAA,QADA;AAEA,QAAA,KAAA,EAAA,KAAA,cAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,iBAAA,CAAA,IAAA,CAAA,SAAA;AAAA,SAAA,EAAA;AAFA,OALA,EASA;AACA,QAAA,IAAA,EAAA,WADA;AAEA,QAAA,KAAA,EAAA,KAAA,cAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,iBAAA,IAAA,CAAA,SAAA;AAAA,SAAA,EAAA;AAFA,OATA,CAAA;AAcA;AAtDA,GAnBA;AA2EA,EAAA,OA3EA,qBA2EA;AAAA;;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,4CAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,MAAA,KAAA,CAAA,SAAA,GAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA;;AACA,MAAA,KAAA,CAAA,YAAA;AACA,KAHA;AAIA;AAhFA,CAAA","sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <Header @add-todo=\"addTodo\"/>\r\n    <div class=\"container\">\r\n    <div class=\"right\">\r\n      <TodosInfo :showOptions=\"getShowOptions\"/>\r\n    </div>   \r\n    <div class=\"left\">\r\n      <Paging :currentPage=\"pageIndex\" :totalPages=\"Math.ceil(filterdTodos.length/itemsPerPage)\" :todos=\"filterdTodos\"  @prev-page=\"prevPage\" @next-page=\"nextPage\" @page-size-changed=\"setPageSize\" />\r\n    </div> \r\n    <TodosList :todos=\"displayedTodos\" />       \r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport Header from './components/header.vue'\r\nimport TodosList from './components/todos-list.vue'\r\nimport TodosInfo from './components/todos-info.vue'\r\nimport Paging from './components/pages-navigation.vue'\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    Header,\r\n    TodosList,\r\n    Paging,\r\n    TodosInfo,\r\n  },\r\n  props:[\"pageSize\", \"todosList\"],\r\n  data(){\r\n    return{\r\n      displayedTodos:[],\r\n      filterdTodos:[],\r\n      pageIndex : 1,\r\n      itemsPerPage:10,\r\n      lastItemIndex :0,\r\n     showOptions:[],\r\n    }\r\n  },\r\n  methods:{\r\n    addTodo(newTodo){\r\n      this.todosList.push(newTodo)\r\n    },\r\n    nextPage:function (){    \r\n      if(this.pageIndex<this.todosList.length/this.itemsPerPage) {\r\n      this.pageIndex ++;\r\n      this.lastItemIndex += parseInt(this.itemsPerPage)\r\n      this.displayTodos()\r\n      }\r\n    },\r\n    prevPage:function (){\r\n      if(this.pageIndex>1){\r\n      this.pageIndex --;\r\n      this.lastItemIndex -= parseInt(this.itemsPerPage)\r\n       this.displayTodos()\r\n      }\r\n    }, \r\n    displayTodos: function(){\r\n      this.displayedTodos = this.filterdTodos.slice(this.lastItemIndex, parseInt(this.lastItemIndex) + parseInt(this.itemsPerPage))\r\n    },\r\n    setPageSize: function(pageSize){\r\n     this.itemsPerPage = pageSize\r\n     this.lastItemIndex = 0\r\n     this.pageIndex = 1\r\n     this.displayTodos()\r\n    },\r\n    showActiveTodos(){\r\n      this.filterdTodos = this.todosList.filter(todo=> !todo.completed)\r\n      this.displayTodos()\r\n    },\r\n    showCompletedTodos(){\r\n      this.filterdTodos = this.todosList.filter(todo=> todo.completed)\r\n      this.displayTodos()\r\n    },\r\n    showAllTodos(){\r\n      this.filterdTodos = this.todosList\r\n      this.displayTodos()\r\n    },\r\n    getShowOptions(){\r\n      return this.showOptions = [\r\n         {\r\n             text:\"all\",\r\n             value : this.todosList.length\r\n         },\r\n         {\r\n             text:\"active\",\r\n             value : this.displayedTodos.filter(todo=> !todo.completed).length\r\n         },\r\n          {\r\n             text:\"completed\",\r\n             value : this.displayedTodos.filter(todo=> todo.completed).length\r\n         },\r\n     ]\r\n    }\r\n  },\r\n  created(){\r\n    axios.get('https://jsonplaceholder.typicode.com/todos').then(res=>{\r\n      this.todosList =  this.filterdTodos = res.data\r\n      this.displayTodos()\r\n      })        \r\n  }  \r\n}\r\n</script>\r\n\r\n<style>\r\nbody{\r\n  margin:0;\r\n  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\r\n}\r\n\r\n.right{\r\n  float: right;\r\n}\r\n.left{\r\n  float: left;\r\n}\r\n.container{\r\n  padding: 24px 12px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src"}]}